* {
  box-sizing: border-box;
  font-family: 'Lato', sans-serif;
}

html,
body {
  margin: 0;
  padding: 0;
}

/* Add styles here! */

strong {
  color: lightseagreen;
}

div.max-width-wrapper {
  background: white;
}

.intro-chunk {
  background: white;
  margin-left: 20px;
  width: 100%;
  padding: 20px 0 0 20px;
}

.title {
  margin-left: 10px;
}

header {
  background: white;
}

.card {
  background: hsl(0deg, 0%, 100%);
  width: 600px;
  border: 2px solid;
  margin-left: 20px;
}

.card p {
  width: 100%;
  padding: 20px 0 0 20px;
}

.intro-chunk p {
  padding: 10px 10px 0 0;
  margin-right: auto;
}

div.max-width-wrapper {
  padding: 0 0 0 50px;
}

section.max-width-wrapper {
  background: gray;
  padding: 50px 50px 50px 50px;
}

section {
  background: gray;
}

.indented-heading {
  width: 300px;
  text-align: center;
  background: hsl(45deg, 100%, 50%);
  padding: 10px;
  font-size: 16px;
  box-shadow: 0 5px 2px hsl(45deg, 100%, 40%);
  margin-left: -10px;
}
